        "serialize-javascript": "^6.0.0",
        "serve-favicon": "^2.5.0",
        "vue": "^3.2.21",
        "vue-router": "^4.0.12",
        "vuex": "^4.0.2",
        "vuex-router-sync": "^5.0.0",
        "webpack": "^5.62.1",
        "webpack-cli": "^4.9.1",
        "webpack-dev-middleware": "^5.2.1",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "devDependencies": {
