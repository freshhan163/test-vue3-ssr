{
    "restartable": "rs",
    "ignore": [
        "src",
        "node_modules",
        "build/*",
        "log/*"
    ],
    "watch": ["server/**/*"],
    "verbose": true,
    "exec": "ts-node --project server/tsconfig.json --files server/app-dev.ts",
    "env": {
        "PORT": "3001"
    },
    "ext": "ts,js"
}
