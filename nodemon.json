{
    "restartable": "rs",
    "ignore": [
        "src",
        "node_modules",
        "build/*",
        "log/*"
    ],
    "watch": ["server/**/*"],
    "verbose": true,
    "exec": "ts-node --project server/tsconfig.json --files server/app.ts",
    "env": {
        "PORT": 8081,
        "DEFAULT_PROXY_SERVER": "http://localhost:3500"
    },
    "ext": "ts,js"
}
